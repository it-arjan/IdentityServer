<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="SiliconClientId" value="dev-silicon"/> 
    <add key="SiliconClientSecret" value="dev-silicon-secret"/>

    <add key="FrontendClientId" value="dev-human"/> 
    <add key="user.bob" value="bob"/> 
    <add key="user.bob.password" value="dev-bob-password"/>
    <add key="user.admin" value="admin"/> 
    <add key="user.admin.password" value="dev-admin-password"/>
    
    <add key="scheme" value="https"/>
    <add key="hostname" value="local.identityserver"/>
    <add key="port" value="5000"/>
